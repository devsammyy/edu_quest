{
  "expo": {
    "name": "quiz_app",
    "slug": "quiz_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "quizapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios"
    },
    "android": {
      "package": "com.talented_dev.quiz_app",
      "bundle": {
        "tool": "gradle"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f1a3b4c-3ebb-4600-aa4a-09188948f0a1"
      }
    }
  }
}
