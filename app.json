{
  "expo": {
    "name": "EduQuest",
    "slug": "EduQuest",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "eduquest",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/etLogo.png",
    "splash": {
      "image": "./assets/images/etLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios"
    },
    "android": {
      "package": "com.talented_dev.quiz_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f1a3b4c-3ebb-4600-aa4a-09188948f0a1"
      }
    },
    "jsEngine": "hermes"
  }
}
